#!/usr/bin/env groovy

import groovy.json.JsonSlurperClassic

def jsonParse(def json) {
    new groovy.json.JsonSlurperClassic().parseText(json)
 }   

pipeline {
	agent any

	stages {

		stage('Terraform: deployment') {

			steps {
                            sh 'terraform -version'
                            sh 'ansible --version'
                         }
                }
       }
 }
